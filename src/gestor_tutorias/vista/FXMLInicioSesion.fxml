<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gestor_tutorias.controlador.FXMLInicioSesionController">
    <children>
        <Pane prefHeight="400.0" prefWidth="300.0">
            <children>
                <ImageView fx:id="IVLogo" fitHeight="209.0" fitWidth="243.0" layoutX="46.0" layoutY="96.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../recurso/Universidad-Veracruzana_logo-01.png" />
                    </image></ImageView>
            </children>
        </Pane>
        <Pane layoutX="300.0" prefHeight="400.0" prefWidth="300.0">
            <children>
                <Label fx:id="lbInicioSesion" layoutX="89.0" layoutY="69.0" text="Iniciar Sesión">
                    <font>
                        <Font size="21.0" />
                    </font></Label>
                <Label fx:id="lbUsuario" layoutX="121.0" layoutY="132.0" text="noPersonal" />
                <Label fx:id="lbPassword" layoutX="120.0" layoutY="200.0" text="Contraseña" />
                <TextField fx:id="tfUsuario" alignment="CENTER" layoutX="76.0" layoutY="149.0" promptText="User123" />
                <PasswordField fx:id="pfPassword" alignment="CENTER" layoutX="76.0" layoutY="220.0" promptText="*****" />
                <Button fx:id="botonIniciar" layoutX="121.0" layoutY="271.0" mnemonicParsing="false" onAction="#clicIniciarSesion" text="Ingresar" />
                <Label fx:id="lbErrorUsuario" alignment="CENTER" contentDisplay="CENTER" layoutX="85.0" layoutY="174.0" prefHeight="17.0" prefWidth="129.0" textAlignment="CENTER" textFill="RED" />
                <Label fx:id="lbErrorPassword" alignment="CENTER" contentDisplay="CENTER" layoutX="33.0" layoutY="245.0" prefHeight="17.0" prefWidth="235.0" textAlignment="CENTER" textFill="RED" wrapText="true" />
            </children>
        </Pane>
    </children>
</AnchorPane>
